package application;

//representation of user 

public class User {
	
	private String cardNumber;
	private String name;
	private String id;
	private String last=null;
	
	@Override
	public String toString(){
		return name;
	}
	
	public User(String id, String name,String number){
		this.id=id;
		this.name=name;
		this.cardNumber=number;
	}
	
	public User setCardNumber(String number){
		cardNumber=number;
		return this;
	}
	
	public User setName(String name){
		this.name=name;
		return this;
	}
	
	public User setId(String id){
		this.id=id;
		return this;
	}
	
	public User setLast(String id){
		this.last=id;
		return this;
	}
	
	public String getName(){
		return name;
	}
	
	public String getId(){
		return id;
	}
	
	public String getCardNumber(){
		return cardNumber;
	}
	public String getLast(){
		return last;
	}
	
}
